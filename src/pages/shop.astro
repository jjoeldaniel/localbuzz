        <Image
            alt="Magnifying glass"
            src={search_icon}
            class="w-18 h-18 left-[755px] top-[322px] absolute rounded-[10px] scale-50"
        />
        <a href="/"><Image alt="LocalBuzz logo" class="w-96 h-32 left-[201px] top-[82px] absolute" src={logo} /></a>
        <a href="/"
            ><Image
                alt="LocalBuzz Bee"
                class="w-28 h-32 left-[96px] top-[79px] absolute transition delay-150 duration-300 ease-in-out hover:-translate-y-1 hover:scale-105"
                src={logo_bee}
            /></a
        >
    </div>
</Layout>
